@model Lab4.Department

@{
    ViewData["Title"] = "Детальніше";
}

<h1>Детальніше</h1>

<div>
    <h4>Кафедра</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.DepartmentName)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.DepartmentName)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Faculty)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Faculty.FacultyName)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model?.Id">Edit</a> |
    @*<a asp-action="Index">Back to List</a>*@
    @Html.ActionLink("Назад до списку кафедр", "Index", new { id = @ViewBag.FacultyId, name = @ViewBag.FacultyName })
</div>
